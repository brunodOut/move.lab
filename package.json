{
  "name": "move.lab",
  "version": "0.0.1",
  "description": "Simple animation plugin for lab.js",
  "private": true,
  "type": "module",
  "dependencies": {
    "@babel/core": "^7.15.0",
    "@babel/plugin-transform-modules-commonjs": "^7.15.0",
    "babel-jest": "^27.1.0",
    "jsdom": "^17.0.0",
    "lab.js": "^20.2.3",
    "lodash": "^4.17.21"
  },
  "jest": {
    "verbose": true,
    "moduleFileExtensions": [
      "mjs",
      "js",
      "jsx",
      "ts",
      "tsx",
      "json",
      "node"
    ],
    "roots": [
      "<rootDir>/test/updaters",
      "<rootDir>/test/plugins",
      "<rootDir>/test"
    ]
  },
  "scripts": {
    "test": "jest --config=jest.config.cjs --verbose --watchAll",
    "build:development": "webpack --mode=development",
    "build:production": "webpack --mode=production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brunodOut/move.lab.git"
  },
  "keywords": [
    "lab.js",
    "animation",
    "cognitive",
    "science",
    "cognition",
    "experiments",
    "experimental",
    "research"
  ],
  "author": "Bruno Moreira-Guedes <moreira.guedes@estudante.uffs.edu.br>",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/brunodOut/move.lab/issues"
  },
  "homepage": "https://github.com/brunodOut/move.lab#readme",
  "devDependencies": {
    "cross-env": "^7.0.3",
    "jest": "^27.0.6",
    "webpack": "^5.51.1",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.8.0"
  }
}
